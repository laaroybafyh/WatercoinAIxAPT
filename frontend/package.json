{
  "name": "watercoin-ai-monitor-pos",
  "version": "0.2.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev -p 3000",
    "test:local": "node --loader ts-node/esm tests/runTests.ts",
    "test:aptos": "node --loader ts-node/esm tests/aptos.test.ts",
    "test:aptos:surveys": "node --loader ts-node/esm tests/survey.test.ts",
    "test:aptos:averages": "node --loader ts-node/esm tests/averages.test.ts",
    "test:aptos:health": "node --loader ts-node/esm tests/health.test.ts",
    "test:aptos:all": "node --loader ts-node/esm tests/aptosAll.test.ts",
    "test:all": "npm run test:local && npm run test:aptos:all",
    "build": "next build",
    "start": "next start -p 3000",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "update-deps": "npm update && npm audit fix"
  },
  "dependencies": {
    "@aptos-labs/ts-sdk": "^1.29.0",
    "@dfinity/agent": "^2.1.2",
    "@dfinity/candid": "^2.1.2",
    "dayjs": "^1.11.13",
    "next": "^15.0.3",
    "qrcode": "^1.5.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "recharts": "^2.13.3",
    "zod": "^3.23.8",
    "zustand": "^5.0.2"
  },
  "devDependencies": {
    "@types/node": "^22.9.1",
    "@types/qrcode": "^1.5.5",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "eslint": "^9.15.0",
    "eslint-config-next": "^15.0.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3"
  },
  "engines": {
    "node": ">=18.17.0",
    "npm": ">=9.0.0"
  }
}
